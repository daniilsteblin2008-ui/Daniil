num 4
i = 11
While i < 30:
  print(i**2)
  i ++2

num 6
a = int(input())
count = 0
while a != 0:
  if a % 2==0:
    count+=1
  a = int(input())
print(count)

num 7
i = int(input())
a = 0
if i != 0:
    count = int(input())
    while count != 0:
        if count > i:
            a += 1
        i = count
        count = int(input())
print(a)
